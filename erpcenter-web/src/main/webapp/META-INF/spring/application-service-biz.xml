<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
           http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	<context:annotation-config />


	<bean id="productTagService" class="com.yihg.product.impl.ProductTagServiceImpl" />
	
	<bean id="productRemarkService" class="com.yihg.product.impl.ProductRemarkServiceImpl" />

	
	<bean id="productRouteService" class="com.yihg.product.impl.ProductRouteServiceImpl" />
	
	<bean id="productRouteSupplierService" class="com.yihg.product.impl.ProductRouteSupplierServiceImpl" />
	
	<bean id="productRouteTrafficService" class="com.yihg.product.impl.ProductRouteTrafficServiceImpl" />
	
	
	<bean id="productInfoService" class="com.yihg.product.impl.ProductInfoServiceImpl"/>
	
	<bean id="productGroupService" class="com.yihg.product.impl.ProductGroupServiceImpl"/>
	
	<bean id="productGroupSupplierService" class="com.yihg.product.impl.ProductGroupSupplierServiceImpl"/>
  	
	<bean id="productGroupPriceService" class="com.yihg.product.impl.ProductGroupPriceServiceImpl"/>
	
	<bean id="productGroupExtraItemService" class="com.yihg.product.impl.ProductGroupExtraItemServiceImpl"/>
    
    <bean id="productAttachmentService" class="com.yihg.product.impl.ProductAttachmentServiceImpl"/>
    
    <bean id="productGroupSellerService" class="com.yihg.product.impl.ProductGroupSellerServiceImpl"/>
	
    <bean id="productStockService" class="com.yihg.product.impl.ProductStockServiceImpl"/>

	
    <bean id="trafficResService" class="com.yihg.product.impl.TrafficResServiceImpl"/>
    
    
    <bean id="trafficResProductService" class="com.yihg.product.impl.TrafficResProductServiceImpl"/>
	

</beans>