<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
       http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
       >
 
  <bean id="productFacadeExport" class="com.alibaba.dubbo.config.spring.ServiceBean">
              <property name="interface" value="com.yimayhd.erpcenter.facade.service.ProductFacade"/>
              <property name="ref" ref="productFacade"/>
              <property name="application" ref="dubboApplicationConfig"/>
              <property name="registry" ref="dubboRegistryConfig"/>
              <property name="protocol" ref="dubboProtocolConfig"/>
              <property name="version" value="${export.dubbo.version}"/>
              <property name="timeout" value="${export.dubbo.timeout}"/>
              <property name="retries" value="${dubbo.retries}"/>
  </bean>
  <bean id="productPricePlusFacadeExport" class="com.alibaba.dubbo.config.spring.ServiceBean">
 			  <property name="interface" value="com.yimayhd.erpcenter.facade.service.ProductPricePlusFacade"/>
              <property name="ref" ref="productPricePlusFacade"/>
              <property name="application" ref="dubboApplicationConfig"/>
              <property name="registry" ref="dubboRegistryConfig"/>
              <property name="protocol" ref="dubboProtocolConfig"/>
              <property name="version" value="${export.dubbo.version}"/>
              <property name="timeout" value="${export.dubbo.timeout}"/>
              <property name="retries" value="${dubbo.retries}"/>
  </bean>
  <bean id="productUpAndDownFrameFacadeExport" class="com.alibaba.dubbo.config.spring.ServiceBean">
 			  <property name="interface" value="com.yimayhd.erpcenter.facade.service.ProductUpAndDownFrameFacade"/>
              <property name="ref" ref="productUpAndDownFrameFacade"/>
              <property name="application" ref="dubboApplicationConfig"/>
              <property name="registry" ref="dubboRegistryConfig"/>
              <property name="protocol" ref="dubboProtocolConfig"/>
              <property name="version" value="${export.dubbo.version}"/>
              <property name="timeout" value="${export.dubbo.timeout}"/>
              <property name="retries" value="${dubbo.retries}"/>
  </bean>
  <bean id="productStockFacadeExport" class="com.alibaba.dubbo.config.spring.ServiceBean">
 			  <property name="interface" value="com.yimayhd.erpcenter.facade.service.ProductStockFacade"/>
              <property name="ref" ref="productStockFacade"/>
              <property name="application" ref="dubboApplicationConfig"/>
              <property name="registry" ref="dubboRegistryConfig"/>
              <property name="protocol" ref="dubboProtocolConfig"/>
              <property name="version" value="${export.dubbo.version}"/>
              <property name="timeout" value="${export.dubbo.timeout}"/>
              <property name="retries" value="${dubbo.retries}"/>
  </bean>
   
</beans>